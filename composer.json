{
    "name": "chrismou/phergie-irc-plugin-react-audioscrobbler",
    "description": "Phergie plugin for returning the current or last played song for a user on last.fm or libre.fm",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "homepage": "http://github.com/chrismou/phergie-irc-plugin-react-audioscrobbler",
    "license": "BSD-2-Clause",
    "support": {
        "email": "phergie@mou.me.uk",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/chrismou/phergie-irc-plugin-react-audioscrobbler/issues",
        "source": "https://github.com/chrismou/phergie-irc-plugin-react-audioscrobbler"
    },
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "dev-master",
        "phergie/phergie-irc-plugin-react-command": "~1.0",
        "wyrihaximus/phergie-http": "~1.0|~2.0",
        "openlss/func-time": "*"

    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "2.0.0-beta2",
        "codeclimate/php-test-reporter": "dev-master",
        "phergie/phergie-irc-plugin-react-pong": "dev-master",
        "phergie/phergie-irc-plugin-react-autojoin": "1.0.*"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Chrismou\\Phergie\\Plugin\\Audioscrobbler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrismou\\Phergie\\Tests\\Plugin\\Audioscrobbler\\": "tests"
        }
    }
}
